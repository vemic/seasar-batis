<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.vemi.seasarbatis">
  <!-- SELECT用のステートメント -->
  <select id="preparedSELECT" parameterType="map" resultType="map"> ${sql} </select>
  <!-- INSERT用のステートメント -->
  <insert id="preparedINSERT" parameterType="map"> ${sql} </insert>
  <!-- UPDATE用のステートメント -->
  <update id="preparedUPDATE" parameterType="map"> ${sql} </update>
  <!-- DELETE用のステートメント -->
  <delete id="preparedDELETE" parameterType="map"> ${sql} </delete>
</mapper>